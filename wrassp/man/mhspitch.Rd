  \item{BeginTime}{bla}

  \item{CenterTime}{bli}

  \item{EndTime}{blup}

  \item{WindowShift}{bla}

  \item{Gender}{bli}

  \item{MaxF}{blup}

  \item{MinF}{bla}

  \item{MinAmp}{bli}

  \item{MinAC1}{blup}

  \item{MinRMS}{bla}

  \item{MaxZCR}{bli}

  \item{MinProb}{blup}

  \item{PlainSpectrum}{bla}

  \item{ToFile}{bli}

  \item{ExplicitExt}{blup}
}
\value{
  nrOfProcessedFiles or if only one file to process return
  dataObj of that file
}
